{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run C++ Project",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/humanoid",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build C++ Project"
        }
    ]
}
